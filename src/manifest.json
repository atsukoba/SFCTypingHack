{
  "name": "SFC-TypingHack",
  "description": "SFC-Typing Hack Extension: incredible speed typing automation",
  "short_name": "SFCTypeHack",
  "version": "0.1.3",
  "manifest_version": 2,
  "author": "atsuyakoba",
  "incognito": "not_allowed",
  "content_scripts": [{
    "matches": ["https://itclass.sfc.keio.ac.jp/typingtest/student/*"],
    "js": ["jquery-2.2.0.min.js",  "setup.js", "hack.js"]
  }],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}